<body>
  <div class="container">
    <header>
      <div>
        <input id="dictName" placeholder="Enter your dictionary name (e.g. Medical Terms)">
        <p class="lead">Personalize the title — it is saved locally for each user.</p>
      </div>
      <div class="controls">
        <button id="btnExportCsv">Export CSV</button>
        <button id="btnImportCsv" class="secondary">Import CSV</button>
        <button id="btnCopyHtml" class="secondary">Copy full HTML</button>
        <button id="btnShare" class="secondary">Share / GitHub guide</button>
      </div>
    </header>

    <section class="card">
      <form id="entryForm" class="row">
        <div>
          <label for="english">English</label>
          <input id="english" type="text" placeholder="word or term" required="">
        </div>
        <div>
          <label for="uzbek">Uzbek</label>
          <input id="uzbek" type="text" placeholder="tarjima" required="">
        </div>
        <div>
          <label for="imageUrl">Image (URL or upload)</label>
          <div style="display:flex;gap:6px">
            <input id="imageUrl" type="url" placeholder="https://example.com/pic.jpg">
            <input id="imageFile" type="file" accept="image/*">
          </div>
        </div>
        <div style="display:flex;flex-direction:column;gap:6px">
          <button id="btnAdd" type="submit" style="min-width:110px">Add</button>
          <button id="btnClearAll" type="button" class="btn-ghost">Clear all</button>
        </div>
      </form>

      <div class="hint">Tip: uploaded images are stored as base64 in localStorage — keep number and size modest to avoid storage limits.</div>
    </section>

    <section class="card" id="tableCard">
      <div style="display:flex;justify-content:space-between;align-items:center">
        <h2 id="titlePreview" style="font-size:1rem;margin:0">sport</h2>
        <div class="hint">Entries: <span id="count">6</span></div>
      </div>

      <div style="overflow:auto">
        <table id="dictTable">
          <thead>
            <tr>
              <th style="width:28%">English</th>
              <th style="width:28%">Uzbek</th>
              <th style="width:18%">Image</th>
              <th style="width:26%">Actions</th>
            </tr>
          </thead>
          <tbody><tr><td><input type="text" class="small"></td><td><input type="text" class="small"></td><td class="hint">—</td><td class="actions"><button class="secondary">Change Image</button><button class="danger">Delete</button></td></tr><tr><td><input type="text" class="small"></td><td><input type="text" class="small"></td><td class="hint">—</td><td class="actions"><button class="secondary">Change Image</button><button class="danger">Delete</button></td></tr><tr><td><input type="text" class="small"></td><td><input type="text" class="small"></td><td class="hint">—</td><td class="actions"><button class="secondary">Change Image</button><button class="danger">Delete</button></td></tr><tr><td><input type="text" class="small"></td><td><input type="text" class="small"></td><td class="hint">—</td><td class="actions"><button class="secondary">Change Image</button><button class="danger">Delete</button></td></tr><tr><td><input type="text" class="small"></td><td><input type="text" class="small"></td><td class="hint">—</td><td class="actions"><button class="secondary">Change Image</button><button class="danger">Delete</button></td></tr><tr><td><input type="text" class="small"></td><td><input type="text" class="small"></td><td><img class="thumb" src="data:image/webp;base64," alt="book"></td><td class="actions"><button class="secondary">Change Image</button><button class="danger">Delete</button></td></tr></tbody>
        </table>
      </div>

      <div class="hint">You can edit entries inline or delete them. Use Export/Import to share via CSV.</div>
    </section>

    <footer>
      <small>Single-file template • Save locally in your browser • Use "Copy full HTML" to paste into a new GitHub repository for GitHub Pages.</small>
    </footer>
  </div>

  <input id="csvInput" type="file" accept="text/csv,text/plain" style="display:none">

  <div id="modal" class="modal-backdrop" role="dialog" aria-hidden="true" style="display: none;">
    <div class="modal" role="document">
      <h3>How to publish this file to GitHub Pages</h3>
      <ol>
        <li>Create a new GitHub repository.</li>
        <li>Add a file named <code>index.html</code> and paste the full HTML content (use "Copy full HTML").</li>
        <li>Commit and go to <strong>Settings → Pages</strong>, set source to main branch (root).</li>
        <li>Visit <code>https://&lt;username&gt;.github.io/&lt;repo-name&gt;/</code> after a minute.</li>
      </ol>
      <p class="hint">Alternatively download the HTML and upload via GitHub web editor or Desktop.</p>
      <div style="display:flex;gap:8px;margin-top:8px">
        <button id="closeModal" class="secondary">Close</button>
      </div>
    </div>
  </div>

  <script>
    const STORAGE_KEY = 'universalDictionaryEntries_v1';
    const NAME_KEY = 'universalDictionaryName_v1';

    // DOM refs
    const dictNameInput = document.getElementById('dictName');
    const titlePreview = document.getElementById('titlePreview');
    const englishInput = document.getElementById('english');
    const uzbekInput = document.getElementById('uzbek');
    const imageUrlInput = document.getElementById('imageUrl');
    const imageFileInput = document.getElementById('imageFile');
    const entryForm = document.getElementById('entryForm');
    const btnClearAll = document.getElementById('btnClearAll');
    const dictTableBody = document.querySelector('#dictTable tbody');
    const countSpan = document.getElementById('count');
    const btnExportCsv = document.getElementById('btnExportCsv');
    const btnImportCsv = document.getElementById('btnImportCsv');
    const csvInput = document.getElementById('csvInput');
    const btnCopyHtml = document.getElementById('btnCopyHtml');
    const btnShare = document.getElementById('btnShare');
    const modal = document.getElementById('modal');
    const closeModal = document.getElementById('closeModal');

    // state
    let entries = [];
    try { entries = JSON.parse(localStorage.getItem(STORAGE_KEY)) || []; } catch(e){ entries = []; }
    dictNameInput.value = localStorage.getItem(NAME_KEY) || '';

    // update title preview
    function updateTitle(){ titlePreview.textContent = dictNameInput.value.trim() || 'Dictionary'; }
    dictNameInput.addEventListener('input', ()=>{
      localStorage.setItem(NAME_KEY, dictNameInput.value.trim());
      updateTitle();
    });
    updateTitle();

    // helpers
    function readEntries(){ try{ return JSON.parse(localStorage.getItem(STORAGE_KEY)) || []; } catch(e){ return []; } }
    function writeEntries(arr){ localStorage.setItem(STORAGE_KEY, JSON.stringify(arr)); renderTable(); }

    function escapeCsvCell(text){
      if(text==null) return '';
      const s = String(text);
      if(s.includes(',') || s.includes('\n') || s.includes('"')){
        return '"' + s.replace(/"/g,'""') + '"';
      }
      return s;
    }

    // rendering
    function renderTable(){
      entries = readEntries();
      dictTableBody.innerHTML = '';
      entries.forEach((e,i)=>{
        const tr = document.createElement('tr');

        const tdEng = document.createElement('td');
        const engInput = document.createElement('input');
        engInput.type = 'text'; engInput.value = e.english || '';
        engInput.className = 'small';
        engInput.addEventListener('change', ()=>{ entries[i].english = engInput.value; writeEntries(entries); });
        tdEng.appendChild(engInput);

        const tdUzb = document.createElement('td');
        const uzInput = document.createElement('input');
        uzInput.type = 'text'; uzInput.value = e.uzbek || '';
        uzInput.className = 'small';
        uzInput.addEventListener('change', ()=>{ entries[i].uzbek = uzInput.value; writeEntries(entries); });
        tdUzb.appendChild(uzInput);

        const tdImg = document.createElement('td');
        if(e.imageData){
          const img = document.createElement('img'); img.className='thumb'; img.src = e.imageData; img.alt = e.english || 'image';
          tdImg.appendChild(img);
        } else if(e.imageUrl){
          const img = document.createElement('img'); img.className='thumb'; img.src = e.imageUrl; img.alt = e.english || 'image';
          img.onerror = ()=>{ img.style.opacity = 0.4; img.title = 'Failed to load'; }
          tdImg.appendChild(img);
        } else { tdImg.textContent = '—'; tdImg.className='hint'; }

        const tdAct = document.createElement('td');
        tdAct.className = 'actions';
        const btnChangeImg = document.createElement('button'); btnChangeImg.textContent='Change Image'; btnChangeImg.className='secondary';
        btnChangeImg.addEventListener('click', ()=> openEditImageDialog(i));
        const btnDelete = document.createElement('button'); btnDelete.textContent='Delete'; btnDelete.className='danger';
        btnDelete.addEventListener('click', ()=>{ if(confirm('Delete this entry?')){ entries.splice(i,1); writeEntries(entries); } });
        tdAct.appendChild(btnChangeImg); tdAct.appendChild(btnDelete);

        tr.appendChild(tdEng); tr.appendChild(tdUzb); tr.appendChild(tdImg); tr.appendChild(tdAct);
        dictTableBody.appendChild(tr);
      });
      countSpan.textContent = entries.length;
    }

    // add entry
    entryForm.addEventListener('submit', async (ev)=>{
      ev.preventDefault();
      const eng = englishInput.value.trim();
      const uz = uzbekInput.value.trim();
      const url = imageUrlInput.value.trim();
      const file = imageFileInput.files[0];
      if(!eng || !uz){ alert('Please enter both English and Uzbek text.'); return; }
      let imageData = null, imageUrl = null;
      if(file){
        try{ imageData = await readFileAsDataURL(file); } catch(e){ console.warn('File read failed',e); }
      } else if(url){ imageUrl = url; }
      entries.push({english:eng, uzbek:uz, imageData, imageUrl, createdAt: Date.now()});
      writeEntries(entries);
      englishInput.value=''; uzbekInput.value=''; imageUrlInput.value=''; imageFileInput.value='';
    });

    btnClearAll.addEventListener('click', ()=>{ if(confirm('Clear all entries from local storage?')){ entries=[]; writeEntries(entries); } });

    function readFileAsDataURL(file){
      return new Promise((res,rej)=>{
        const r = new FileReader();
        r.onload = ()=>res(r.result);
        r.onerror = ()=>rej(r.error);
        r.readAsDataURL(file);
      });
    }

    async function openEditImageDialog(index){
      const choice = prompt('Enter image URL to set, or leave blank to upload a file.');
      if(choice===null) return;
      if(choice.trim()){
        entries[index].imageUrl = choice.trim();
        entries[index].imageData = null;
        writeEntries(entries);
        return;
      }
      // upload file
      const file = await chooseFile();
      if(file){ const data = await readFileAsDataURL(file); entries[index].imageData = data; entries[index].imageUrl = null; writeEntries(entries); }
    }
    function chooseFile(){ return new Promise(res=>{ const input = document.createElement('input'); input.type='file'; input.accept='image/*'; input.onchange = ()=>res(input.files[0] || null); input.click(); }); }

    // CSV export/import
    btnExportCsv.addEventListener('click', ()=>{
      const arr = readEntries();
      const header = ['english','uzbek','imageUrl','imageData'];
      const lines = [header.join(',')];
      arr.forEach(e=>{
        lines.push([escapeCsvCell(e.english), escapeCsvCell(e.uzbek), escapeCsvCell(e.imageUrl || ''), escapeCsvCell(e.imageData || '')].join(','));
      });
      const blob = new Blob([lines.join('\n')], {type:'text/csv;charset=utf-8;'});
      const url = URL.createObjectURL(blob);
      const a = document.createElement('a'); a.href = url; a.download = (dictNameInput.value.trim() || 'dictionary') + '_export.csv';
      document.body.appendChild(a); a.click(); a.remove(); URL.revokeObjectURL(url);
    });

    btnImportCsv.addEventListener('click', ()=> csvInput.click());
    csvInput.addEventListener('change', (ev)=>{
      const file = ev.target.files[0]; if(!file) return;
      const reader = new FileReader(); reader.onload = ()=>{ try{ parseCsvText(reader.result); } catch(e){ alert('Failed to read CSV: '+e.message); } };
      reader.readAsText(file); csvInput.value='';
    });

    function parseCsvText(text){
      const lines = text.split(/\r?\n/).filter(l=>l.trim()!==''); if(lines.length<=1) { alert('No data found'); return; }
      // assume first line header
      const rows = [];
      for(let i=1;i<lines.length;i++){ const row = splitCsvLine(lines[i]); if(row.length) rows.push(row); }
      const newEntries = rows.map(r=>({english:r[0]||'', uzbek:r[1]||'', imageUrl:r[2]||'', imageData:r[3]||''}));
      if(newEntries.length){
        if(confirm('Append '+newEntries.length+' entries? Cancel to replace.')){ entries = readEntries().concat(newEntries); } else entries = newEntries;
        writeEntries(entries); alert('Imported '+newEntries.length+' entries.'); 
      } else alert('No valid rows found in CSV.');
    }

    function splitCsvLine(line){
      const cells = []; let cur=''; let inQuotes=false;
      for(let i=0;i<line.length;i++){ const ch=line[i];
        if(ch==='"'){ if(inQuotes && line[i+1]==='"'){ cur+="\""; i++; continue;} inQuotes=!inQuotes; continue; }
        if(ch===',' && !inQuotes){ cells.push(cur); cur=''; continue; }
        cur+=ch;
      }
      cells.push(cur); return cells.map(c=>c.replace(/^\s+|\s+$/g,''));
    }

    // copy full html
    btnCopyHtml.addEventListener('click', async ()=>{
      try{ const full = '<!doctype html>\n' + document.documentElement.outerHTML; await navigator.clipboard.writeText(full); alert('Full HTML copied to clipboard. Paste into index.html in a GitHub repo.'); }
      catch(e){ alert('Copy failed: '+e.message); }
    });

    // share modal
    btnShare.addEventListener('click', ()=>{ modal.style.display='flex'; modal.setAttribute('aria-hidden','false'); });
    closeModal.addEventListener('click', ()=>{ modal.style.display='none'; modal.setAttribute('aria-hidden','true'); });
    modal.addEventListener('click', (e)=>{ if(e.target===modal){ modal.style.display='none'; modal.setAttribute('aria-hidden','true'); } });
    document.addEventListener('keydown', (e)=>{ if(e.key==='Escape' && modal.style.display==='flex'){ modal.style.display='none'; modal.setAttribute('aria-hidden','true'); } });

    // init
    renderTable();
  </script>


</body></html>
